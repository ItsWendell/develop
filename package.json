{
  "name": "electron-react-app",
  "description": "A boilerplate for building upon Electron with clean ES6 code.",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron/index.js",
  "dependencies": {
    "concurrently": "^4.0.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "electron": "^3.0.5",
    "electron-builder": "^20.28.4",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "react-app-rewired": "^1.6.2"
  },
  "scripts": {
    "start": "babel-node .electron-react/dev-runner.js",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "electron": "electron -r babel-register .",
    "react-app": "react-app-rewired start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "env",
      "stage-0"
    ]
  }
}